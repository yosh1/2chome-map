  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:ios": "ionic capacitor build ios",
    "build:android": "ionic capacitor build android",
    "start": "next start",
    "lint": "next lint",
    "static": "next build && next export",
    "export": "yarn build && npx cap sync",
    "release": "yarn ver && yarn license && yarn build && npx cap sync",
    "open:ios": "npx cap open ios",
    "open:android": "npx cap open android",
    "res:ios": "cordova-res ios --skip-config --copy",
    "res:android": "cordova-res android --skip-config --copy",
    "live:ios": "ionic cap run ios -l --external",
    "live:android": "ionic cap run android -l --external",
    "prettier": "prettier --write src/**",
    "rules:firestore": "firebase deploy --only firestore:rules",
    "rules:storage": "firebase deploy --only database",
    "license": "license-checker --production --json --out src/assets/license.json --production",
